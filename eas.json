{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env":{"APP_VARIANT": "development"},
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "env":{"APP_VARIANT": "preview"}
    },
    "production": {
      "autoIncrement": true
    },
    "ios-simulator":{
      "extends": "development",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./stickersmash-455508-7c97491bf155.json",
        "track":"internal"
      }
    }
  }
}
